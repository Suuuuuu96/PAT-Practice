//https://pintia.cn/problem-sets/994805342720868352/problems/994805439202443264
#include<iostream>
#define N 100010
using namespace std;
int main(){
	long i,j,k=1,a[N]={0},mi=100000000,sum=0;
	long n,m;
	cin>>n>>m;
	for(i=1;i<=n;i++)
		cin>>a[i];
	i=1;
	j=1;
	sum=a[1];
	while(i<=n&&j<=n){
		if(mi&&sum>=m&&mi>sum-m)
			mi=sum-m;
		if(sum==m){
			printf("%ld-%ld\n",i,j);
			sum-=a[i++];
		}
		else if(sum<m)
			sum+=a[++j];
		else 
			sum-=a[i++];
	}
	if(!mi)
		return 0;
	i=1;
	j=1;
	sum=a[1];
	while(i<=n&&j<=n){
		if(mi==sum-m)
			printf("%ld-%ld\n",i,j);
		if(sum<m)
			sum+=a[++j];
		else 
			sum-=a[i++];
	}
	return 0;
}
/*
16 15
3 2 1 5 4 6 8 7 16 10 15 11 9 12 14 13
5 13
2 4 5 7 9
*/
