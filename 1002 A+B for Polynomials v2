//https://pintia.cn/problem-sets/994805342720868352/problems/994805528788582400
#include<iostream>
#include<string>
using namespace std;
int main()
{
	float a[1200]={0},b[1200]={0},c[1200]={0};
	int na,nb,i,j,n,num=0,k;
	cin>>na;
	for(i=1;i<=na;i++)
	{cin>>j;cin>>a[j];}
	cin>>nb;
	for(i=1;i<=nb;i++)
	{cin>>j;cin>>b[j];}
	for(i=0;i<=1000;i++)
	{c[i]=a[i]+b[i];
	if(c[i]!=0)num++;
	}
	printf("%d",num);
	for(i=1000;i>=0;i--)
	if(c[i]!=0)
	{
		printf(" %d %.1f",i,c[i]);
	}
	return 0;
}
/*
2 1 2.4 0 3.2
2 2 1.5 1 0.5
5 10 2.4 0 3.2 2 3.2 4 3.2 6 3.2
1 1 0.5
*/
