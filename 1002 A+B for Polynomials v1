//https://pintia.cn/problem-sets/994805342720868352/problems/994805528788582400
#include<iostream>
#include<string>

using namespace std;
int main()
{
	float a[12]={0},b[12]={0},c[12]={0};
	int na,nb,i,j,n,num=0,k;
	cin>>na;
	for(i=1;i<=na;i++)
	{cin>>j;cin>>a[j];}
	cin>>nb;
	for(i=1;i<=nb;i++)
	{cin>>j;cin>>b[j];}
	//cin>>a>>b;
	string st0,st=to_string(b[j]);
	j=0;
	for(i=0;i<=st.size()-1;i++){
		if(j&&st[i]!='0')j++;
		if(j&&st[i]=='0'&&st[i-1]=='0')break;
		if(st[i]=='.')j=1;
	}
	j--;
	n=na>nb?na:nb;
	for(i=0;i<=10;i++)
	{c[i]=a[i]+b[i];
	if(c[i]>0.001||c[i]<-0.001)num++;
	}
	printf("%d",num);
	//j=3;
	for(i=10;i>=0;i--)
	if(c[i]>0.001||c[i]<-0.001)
	{
		st0=to_string(c[i]);
		//st0[j]='/0';
		
		printf(" %d ",i);
		for(k=0;k<st0.size();k++)
			if(st0[k]=='.')break;
		string s1(st0,0,k+j+1); 
		cout<<s1;
		
		
	}
	return 0;
}
/*
2 1 2.4 0 3.2
2 2 1.5 1 0.5
*/
